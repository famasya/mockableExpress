{
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "jsx-a11y/href-no-hash": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "no-console": "off"
  },
  "env": {
    "es6": false,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb-base/legacy",
    "prettier"
  ]
}


